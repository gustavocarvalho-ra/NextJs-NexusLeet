generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        String    @id @default(cuid())
  name      String 
  email     String    @unique
  password  String
  role      Role      @default(USER)
  createdAt DateTime  @default(now())
}

